@{
    ViewData["Title"] = "Login Pag";
}
<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
</div>
<div>
    <form method="post" action="/Home/Index" enctype="multipart/form-data">
        <div>
            <div class="form-group">
                <div>
                    <label name="Account" class="control-label">帳號</label>
                    <input type="text" name="Account" class="form-control">
                </div>
                <div>
                    <label name="Password" class="control-label">密碼</label>
                    <input type="text" name="Password" class="form-control">
                </div>
            </div>
            <input type="submit" value="登入" class="btn btn-primary">
            @Html.ActionLink("返回","Index",new{},new { @class = "btn btn-secondary" })
        </div>
        
    </form>
</div>
